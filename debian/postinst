#!/bin/bash -e

#SAFERM#

case "$1" in
	configure)

		mysql -u root -A yabsdb < /usr/share/mysql-bit-strings-udf/drop_funcs.sql
		mysql -u root -A yabsdb < /usr/share/mysql-bit-strings-udf/create_funcs.sql

	;;

	abort-upgrade|abort-remove|abort-deconfigure)
	;;

	*)
		echo "postinst called with unknown argument \`$1'" >&2
		exit 1
	;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.
#DEBHELPER#

exit 0


